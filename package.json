    "start": "node server.js",
    "client:dev": "cd client && yarn start",
    "server:dev": "nodemon",
    "heroku-postbuild": "yarn build",
    "start:dev": "concurrently -n client,server \"yarn run client:dev\" \"yarn run server:dev\""
